@model List<Category>
<div class="container">
	<div class="row pt-4">
		<div class="col-6">
		</div>
		<div class="col-6 text-end">
			<a asp-controller="Category" asp-action="Create" class="btn btn-primary">
				Create New Category
			</a>
		</div>
	</div>
</div>
<h1>Category List</h1>
<table class="table table-bordered table-striped">
	<thead>
	<tr>
		<th>
			Id
		</th>
		<th>
			Name
		</th>
		<th>
			Display Order
		</th>
	</tr>
	<thead>
	<tbody>
		@foreach(var obj in Model)
				{
					<tr>
						<td>@obj.Id</td>
						<td>@obj.Name</td>
						<td>@obj.DisplayOrder</td>
					</tr>
				}
			</tbody>
</table>